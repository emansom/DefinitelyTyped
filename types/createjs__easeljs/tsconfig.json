{
	"compilerOptions": {
		"module": "commonjs",
		"lib": [
			"es6",
			"dom"
		],
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": false,
		"strictFunctionTypes": true,
		"baseUrl": "../",
		"typeRoots": [
			"../"
		],
		"paths": {
			"@createjs/easeljs": ["createjs__easeljs"],
			"@createjs/easeljs/*": ["createjs__easeljs/*"],
			"@createjs/tweenjs": ["createjs__tweenjs"],
			"@createjs/tweenjs/*": ["createjs__tweenjs/*"],
			"@createjs/core": ["createjs__core"],
			"@createjs/core/*": ["createjs__core/*"]
        },
		"types": [],
		"noEmit": true,
		"forceConsistentCasingInFileNames": true
	},
	"files": [
		"index.d.ts",
		"createjs__easeljs-tests.ts"
	]
}
